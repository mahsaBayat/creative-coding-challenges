{
    "name": "bro-template-node",
    "version": "0.0.0",
    "description": "Bro template for creating Node.js projects with Codeship deployments.",
    "main": "lib/index.js",
    "scripts": {
        "build": "webpack --config=support/webpack/webpack.config.js",
        "codeship:decrypt": "./deployment/crypto.sh decrypt ./deployment/encrypted ./deployment/secrets",
        "codeship:encrypt": "./deployment/crypto.sh encrypt ./deployment/secrets ./deployment/encrypted",
        "codeship:local": "jet steps --key-path deployment/codeship.aes",
        "dev": "webpack-dev-server --config=support/webpack/webpack.config.dev.js",
        "dev-ship": "webpack-dev-server --config=support/webpack/webpack.config.mothership.js",
        "story": "start-storybook -p 9001 -c support/storybook",
        "test": "npm-run-all test:*",
        "test:lint": "eslint src",
        "test:unit": "jest"
    },
    "author": "Christopher Turner <ct@propelleraero.com.au>",
    "license": "UNLICENSED",
    "devDependencies": {
        "@storybook/react": "^3.3.14",
        "@types/jest": "^22.1.3",
        "@types/node": "^9.4.6",
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.0",
        "babel-eslint": "^8.2.2",
        "babel-jest": "^22.4.1",
        "babel-loader": "^7.1.3",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-preset-env": "^1.6.1",
        "babel-preset-react": "^6.24.1",
        "enzyme": "^3.3.0",
        "enzyme-adapter-react-16": "^1.1.1",
        "eslint": "^4.18.1",
        "eslint-config-airbnb": "^16.1.0",
        "eslint-plugin-import": "^2.9.0",
        "eslint-plugin-jsx-a11y": "^6.0.3",
        "eslint-plugin-react": "^7.7.0",
        "global": "^4.3.2",
        "jest": "^22.4.2",
        "npm-run-all": "^4.1.2",
        "react-hot-loader": "^4.0.0",
        "webpack": "^4.0.1",
        "webpack-cli": "^2.0.10",
        "webpack-dev-server": "^3.1.0",
        "webpack-manifest-plugin": "^2.0.0-rc.2",
        "webpack-merge": "^4.1.2"
    },
    "dependencies": {
        "prop-types": "^15.6.1",
        "react": "^16.2.0",
        "react-dom": "^16.2.0",
        "react-redux": "^5.0.7",
        "redux": "^3.7.2",
        "redux-thunk": "^2.2.0",
        "reselect": "^3.0.1"
    }
}